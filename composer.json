{
    "name": "laravel-russian/laravel-language",
    "description": "Language switcher package for Laravel",
    "keywords": [
        "laravel",
        "language",
        "switcher"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "mltefive",
            "email": "mltefive@gmail.com",
            "homepage": "https://laravel-russian.com",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "^8|^9|^10",
        "jenssegers/agent": ">=2.6.*"
    },
    "autoload": {
        "psr-4": {
            "LaravelRussian\\Language\\": "./src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelRussian\\Language\\Provider"
            ],
            "aliases": {
                "Language": "LaravelRussian\\Language\\Facade"
            }
        }
    }
}
